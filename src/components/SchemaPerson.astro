---
export interface Props {
  id: string
  givenName: string
  familyName: string
}

const { id, givenName, familyName } = Astro.props as Props

const ldData = {
  '@type': 'Person',
  '@id': id,
  givenName,
  familyName
}
---

<script type="application/ld+json" set:html={JSON.stringify(ldData, null, 2)}
></script>
