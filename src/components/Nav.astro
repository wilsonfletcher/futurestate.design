---
import Button from './Button.vue';

export interface Props {
  nav?: any
}

const {
  nav = {
    id: 'primary',
    name: 'Primary'
  }
} = Astro.props
// const url = new URL(Astro.request.url);
// const foo = Astro.canonicalURL
---

<nav aria-labelledby={nav.id}>
  <h2 id={nav.id} class="hidden">{nav.name}</h2>
  <ul class="flex lg:gap-5">
    {nav.items.map(item =><li><Button as="a" href={item.url} class="text-white">{item.name}</Button></li>)}
  </ul>
</nav>
